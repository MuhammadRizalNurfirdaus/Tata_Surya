<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient android:startColor="#55FF9800" android:endColor="#11FFFFFF" android:angle="135"/>
        </shape>
    </item>
    <item android:inset="3dp">
        <shape android:shape="oval">
            <solid android:color="@color/space_surface"/>
            <stroke android:width="2dp" android:color="@color/space_accent_soft"/>
        </shape>
    </item>
</layer-list>

